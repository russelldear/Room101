{
  "manifest_version": 2,

  "name": "Binary Converter",
  "description": "Automatically append an ascii translation to any binary code.",
  "version": "1.1",

  "permissions": [
     "tabs", "http://*/*"
  ],
  "background": "background.html",
  "content_scripts": [ {
        "all_frames": true,
        "matches": ["*://*/*"],
        "js": [ "jquery-1.9.1.min.js", "content.js" ],
        "run_at": "document_end"
  } ]
}

